-- Scripts:

local function QQGB_fake_script() -- destroy.LocalScript 
	local script = Instance.new('LocalScript', destroy)

	script.Parent.MouseButton1Click:Connect(function()
	
	Actual:Destroy()
	
	end)
end
coroutine.wrap(QQGB_fake_script)()
local function LZYPVKV_fake_script() -- Bolts.LocalScript 
	local script = Instance.new('LocalScript', Bolts)

	script.Parent.MouseButton1Click:Connect(function()
	
		loadstring(game:HttpGet("https://raw.githubusercontent.com/timcheno3o/chicken-hub-parody-/main/bolt"))()
	
	
	end)
end
coroutine.wrap(LZYPVKV_fake_script)()
