coroutine.wrap(CTKUO_fake_script)()
local function TVRVGF_fake_script() -- Creditbutton.LocalScript 
	local script = Instance.new('LocalScript', Creditbutton)

	script.Parent.MouseButton1Click:Connect(function()
			Tab.Visible = false
			Main.Visible = false
			Credits.Visible = true
	end)
end

function onKeyPress(inputObject, gameProcessedEvent)
	if not gameProcessedEvent then
		if inputObject.KeyCode == Enum.KeyCode.RightShift  then 
			Actual.Visible = not Actual.Visible
		end
	end
end
game:GetService("UserInputService").InputBegan:connect(onKeyPress)
coroutine.wrap(TVRVGF_fake_script)()
